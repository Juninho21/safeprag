# Configurações do Supabase
# Copie este arquivo para .env e preencha com suas chaves reais

# URL do projeto Supabase
VITE_SUPABASE_URL=https://badyvhzrjbemyqzeqlaw.supabase.co

# Chave anônima do Supabase (encontre no painel do Supabase > Settings > API)
VITE_SUPABASE_ANON_KEY=sua_chave_anon_aqui

# Firebase
VITE_FIREBASE_API_KEY="sua_api_key_aqui"
VITE_FIREBASE_AUTH_DOMAIN="seu_auth_domain_aqui"
VITE_FIREBASE_PROJECT_ID="seu_project_id_aqui"
VITE_FIREBASE_STORAGE_BUCKET="seu_storage_bucket_aqui"
VITE_FIREBASE_MESSAGING_SENDER_ID="seu_messaging_sender_id_aqui"
VITE_FIREBASE_APP_ID="seu_app_id_aqui"
VITE_FIREBASE_MEASUREMENT_ID="seu_measurement_id_aqui"

# Outras configurações
VITE_APP_ENV=development
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_OFFLINE_MODE=true